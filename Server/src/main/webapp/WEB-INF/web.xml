	<servlet>
		<servlet-name>QueryAllServlet</servlet-name>
		<servlet-class>com.server.crud.QueryAllServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QueryAllServlet</servlet-name>
		<url-pattern>/queryAll</url-pattern>
	</servlet-mapping>
</web-app>
