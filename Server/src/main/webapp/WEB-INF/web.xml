<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>InsertServlet</servlet-name>
		<servlet-class>com.server.crud.InsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InsertServlet</servlet-name>
		<url-pattern>/insert</url-pattern>
